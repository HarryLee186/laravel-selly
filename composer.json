{
    "name": "mccaulay/laravel-selly",
    "description": "Laravel PHP library for Selly's API",
    "keywords": [
        "mccaulay",
        "laravel-selly",
        "laravel",
        "selly",
        "selly.gg",
        "bitcoin",
        "ethereum",
        "paypal",
        "litecoin",
        "ripple",
        "bitcoin cash",
        "payment",
        "payment processing"
    ],
    "homepage": "https://github.com/McCaulay/laravel-selly",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "McCaulay Hudson",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "McCaulay\\Selly\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McCaulay\\Selly\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "McCaulay\\Selly\\Providers\\SellyServiceProvider"
            ],
            "aliases": {
                "Bitcoin": "McCaulay\\Selly\\Facades\\Selly"
            }
        }
    }
}